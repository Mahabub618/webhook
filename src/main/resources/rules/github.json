{
  "source": "github",
  "repoName": "$.repository.full_name",
  "repoUrl": "$.repository.html_url",
  "branchName": "$.ref",
  "authorName": {
    "compose": [
      "$.head_commit.author.name",
      " <",
      "$.head_commit.author.email",
      ">"
    ]
  },
  "avatarUrl": "",
  "commits": {
    "listPath": "$.commits[*]",
    "idField": "id",
    "fields": {
      "message": "message",
      "commitUrl": "url"
    }
  }
}
